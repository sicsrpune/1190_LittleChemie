<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.littlechemie"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.littlechemie.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.littlechemie.NewGame"
            android:label="@string/title_activity_new_game" >
        </activity>
        <activity
            android:name="com.example.littlechemie.Action"
            android:label="@string/title_activity_action" >
        </activity>
        <activity
            android:name="com.example.littlechemie.Play"
            android:label="@string/title_activity_play" >
        </activity>
        <activity
            android:name="com.example.littlechemie.ViewPoints"
            android:label="@string/title_activity_view_points" >
        </activity>
        <activity
            android:name="com.example.littlechemie.SingleItemView"
            android:label="@string/title_activity_singleitemview" >
        </activity>
        <activity
            android:name="com.example.littlechemie.CheckForItem"
            android:label="@string/title_activity_checkitemview" >
        </activity>
    </application>

</manifest>
